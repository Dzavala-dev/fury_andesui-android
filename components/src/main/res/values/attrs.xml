<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="AndesFeedbackInApp" tools:ignore="ResourceName">

        <attr name="andesFeedbackInAppHierarchy" format="enum">
            <enum name="loud" value="100" />
            <enum name="quiet" value="101" />
            <enum name="transparent" value="102" />
        </attr>

        <attr name="andesFeedbackInAppSize" format="enum">
            <enum name="large" value="200" />
            <enum name="medium" value="201" />
            <enum name="small" value="202" />
        </attr>

        <attr name="andesFeedbackInAppAlign" format="enum">
            <enum name="left" value="1000" />
            <enum name="right" value="1001" />
            <enum name="center" value="1002" />
        </attr>

        <attr name="andesFIAShowTopDivider" format="boolean" />

        <attr name="andesFeedbackInAppBodyText" format="reference" />
        <attr name="andesFeedbackInAppButtonText" format="reference" />


    </declare-styleable>

</resources>